package com.chejet.cloud.common;

/**
 * @Author: Bean.Wang
 * @Date: 2018/12/28 11:33
 */
public enum ScopeType {
    /**
     * 管理范围type
     */
    SECTION_SCOPE(0, "部分指定部门"),
    ALL_SCOOPE(1, "全公司"),
    CURRENT_SCOPE(2, "所在及其下级部门");


    private Integer value;
    private String name;

    ScopeType(Integer value, String name) {
        this.value = value;
        this.name = name;
    }

    public Integer getValue() {
        return value;
    }

    public void setValue(Integer value) {
        this.value = value;
    }

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }
}
