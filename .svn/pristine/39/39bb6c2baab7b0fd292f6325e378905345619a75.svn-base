package com.chejet.cloud.dto;

import java.util.Set;

/**
 * @author Neo.fang
 * @creatTime 2018/12/15 0015
 */
public class UserGroup {

    /**
     * 租户ID
     */
    private Long tenantId;

    /**
     * 所属角色类型
     */
    private Integer roleType;

    /**
     * 租户名称
     */
    private String tenantName;

    /**
     * 所在企业公司列表(不要包含直接并列，公司级别适用于应用登录)(该字段弃用)
     */
    private Set companySet;

    /**
     * 公司ID
     */
    private Long companyId;

    /**
     * 公司名称
     */
    private String companyName;

    /**
     * 是否当前选择
     */
    private boolean currentChoice;

    public boolean isCurrentChoice() {
        return currentChoice;
    }

    public void setCurrentChoice(boolean currentChoice) {
        this.currentChoice = currentChoice;
    }

    public Long getTenantId() {
        return tenantId;
    }

    public void setTenantId(Long tenantId) {
        this.tenantId = tenantId;
    }

    public Integer getRoleType() {
        return roleType;
    }

    public void setRoleType(Integer roleType) {
        this.roleType = roleType;
    }

    public String getTenantName() {
        return tenantName;
    }

    public void setTenantName(String tenantName) {
        this.tenantName = tenantName;
    }


    public Long getCompanyId() {
        return companyId;
    }

    public void setCompanyId(Long companyId) {
        this.companyId = companyId;
    }

    public String getCompanyName() {
        return companyName;
    }

    public void setCompanyName(String companyName) {
        this.companyName = companyName;
    }
}
